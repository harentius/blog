{
  "name" : "harentius/blog-bundle",
  "type": "symfony-bundle",
  "description": "Simple Symfony blog/cms",
  "keywords": ["blog", "bundle", "symfony", "cms"],
  "homepage": "https://folkprog.net/symfony-blog-bundle/",
  "license": "MIT",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Yaroslav Osypenko",
      "email": "yaroslav.osypenko@gmail.com",
      "homepage": "https://folkprog.net"
    }
  ],
  "autoload" : {
    "psr-4": { "Harentius\\BlogBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": {
      "BlogBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.0",
    "symfony/symfony": "^4.4",
    "stof/doctrine-extensions-bundle": "^1.3",
    "sonata-project/admin-bundle": "^3.54",
    "sonata-project/doctrine-orm-admin-bundle": "^3.12",
    "sonata-project/translation-bundle": "^2.4",
    "doctrine/orm": "^2.7",
    "doctrine/doctrine-bundle": "^1.4",
    "symfony/swiftmailer-bundle": "^3.4",
    "symfony/monolog-bundle": "^3.5",
    "sensio/framework-extra-bundle": "^5.5",
    "knplabs/knp-paginator-bundle": "^4.1",
    "hautelook/alice-bundle": "^2.5",
    "knplabs/knp-menu-bundle": "^2.3",
    "whiteoctober/breadcrumbs-bundle": "^1.4",
    "beberlei/DoctrineExtensions": "^1.2",
    "friendsofsymfony/jsrouting-bundle": "^2.4",
    "presta/sitemap-bundle": "^1.7",
    "symfony-cmf/routing-bundle": "^2.1",
    "eko/feedbundle": "^2.0",
    "liip/imagine-bundle": "^2.2",
    "monolog/monolog": "^1.22"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5",
    "bamarni/composer-bin-plugin": "^1.3"
  },
  "extra": {
    "bamarni-bin": {
      "bin-links": false
    }
  },
  "scripts": {
    "start": "docker-compose -f support/ci/docker-compose.yml -f support/dev/docker-compose.yml --project-directory=. up --build"
  }
}
