{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Harentius\\BlogBundle\\": "src/Harentius/BlogBundle/src",
            "": "src/",
            "SymfonyStandard\\": "app/"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": "^7.0",
        "doctrine/doctrine-migrations-bundle": "^1.0",
        "harentius/widgets-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.5",
        "symfony/asset": "^4.4",
        "symfony/console": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/security-bundle": "^4.4",
        "symfony/translation": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "twig/twig": "^2.12|^3.0",
        "symfony/twig-pack": "*",
        "symfony/yaml": "4.4.*",
        "stof/doctrine-extensions-bundle": "^1.3",
        "sonata-project/admin-bundle": "^3.54",
        "sonata-project/doctrine-orm-admin-bundle": "^3.12",
        "sonata-project/translation-bundle": "^2.4",
        "doctrine/orm": "^2.7",
        "doctrine/doctrine-bundle": "^1.12 || ^2.0",
        "sensio/framework-extra-bundle": "^5.5",
        "knplabs/knp-paginator-bundle": "^4.1 || ^5.0",
        "hautelook/alice-bundle": "^2.5",
        "knplabs/knp-menu-bundle": "^2.3",
        "whiteoctober/breadcrumbs-bundle": "^1.4",
        "beberlei/doctrineextensions": "^1.2",
        "friendsofsymfony/jsrouting-bundle": "^2.4",
        "presta/sitemap-bundle": "^1.7",
        "symfony-cmf/routing-bundle": "^2.1",
        "eko/feedbundle": "^2.0",
        "liip/imagine-bundle": "^2.2",
        "monolog/monolog": "^1.22"
    },
    "scripts": {
        "cs-check": "vendor-bin/php-cs-fixer/vendor/bin/php-cs-fixer fix --dry-run --diff -v --diff-format=udiff --ansi",
        "cs-fix": "vendor-bin/php-cs-fixer/vendor/bin/php-cs-fixer fix --diff -v --diff-format=udiff --ansi"
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "branch-alias": {
            "dev-master": "2.7-dev",
            "dev-ckeditor-4": "4.0-dev"
        },
        "bamarni-bin": {
            "bin-links": false
        }
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.3"
    }
}
